                android:text="Three_four"
                android:gravity="center"
                android:textColor="@color/colorPrimary"
                android:layout_width="wrap_content"
                android:layout_height="fill_parent"/>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/LinearLayout_Four"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="horizontal"
            android:layout_weight="1"
            >
            <TextView
                android:id="@+id/Four_One"
                android:layout_weight="1"
                android:background="@drawable/border"
                android:layout_marginRight="10dp"
                android:layout_marginBottom="5dp"
                android:text="Four_one"
                android:gravity="center"
                android:textColor="@color/colorPrimary"
                android:layout_width="wrap_content"
                android:layout_height="fill_parent"/>
            <TextView
                android:id="@+id/Four_Two"
                android:layout_weight="2"
                android:background="@drawable/border"
                android:layout_marginRight="10dp"
                android:layout_marginBottom="5dp"
                android:text="Four_two"
                android:gravity="center"
                android:textColor="@color/colorPrimary"
                android:layout_width="wrap_content"
                android:layout_height="fill_parent"/>
            <TextView
                android:id="@+id/Four_Three"
                android:layout_weight="1"
                android:background="@drawable/border"
                android:layout_marginRight="10dp"
                android:layout_marginBottom="5dp"
                android:text="Four_three"
                android:gravity="center"
                android:textColor="@color/colorPrimary"
                android:layout_width="wrap_content"
                android:layout_height="fill_parent"/>
            <TextView
                android:id="@+id/Four_Four"
                android:layout_weight="1"
                android:background="@drawable/border"
                android:layout_marginRight="10dp"
                android:layout_marginBottom="5dp"
                android:text="Four_four"
                android:gravity="center"
                android:textColor="@color/colorPrimary"
                android:layout_width="wrap_content"
